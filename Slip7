//Ceiling fan :


class CeilingFan {
    boolean isOn = false;

    void on() {
        isOn = true;
        System.out.println("Fan is ON");
    }

    void off() {
        isOn = false;
        System.out.println("Fan is OFF");
    }

    void undo() {
        if (isOn) off();
        else on();
    }
}

public class Main {
    public static void main(String[] args) {
        CeilingFan fan = new CeilingFan();

        fan.on();
        fan.undo();
        fan.off();
        fan.undo();
    }
}
